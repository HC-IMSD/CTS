    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>


import java.io.IOException;
import java.io.StringReader;
import java.io.StringWriter;

import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import javax.xml.parsers.ParserConfigurationException;
import javax.xml.transform.OutputKeys;
import javax.xml.transform.Transformer;
import javax.xml.transform.TransformerConfigurationException;
import javax.xml.transform.TransformerException;
import javax.xml.transform.TransformerFactory;
import javax.xml.transform.TransformerFactoryConfigurationError;
import javax.xml.transform.dom.DOMSource;
import javax.xml.transform.stream.StreamResult;


import org.w3c.dom.CharacterData;
import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;
import org.xml.sax.InputSource;
import org.apache.commons.lang3.StringEscapeUtils;
//import org.apache.commons.text.StringEscapeUtils;


String xml="&lt;SUS_PERSON>&lt;/SUS_PERSON>";
String xmlString;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();


    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

			
	String GIVEN_NAME_format = get(Fields.In, "GIVEN_NAME_format").getString(r);
	String SURNAME_format = get(Fields.In, "SURNAME_format").getString(r);
	String TELEPHONE_NUMBER = get(Fields.In, "TELEPHONE_NUMBER").getString(r);
	String FAX_NUMBER = get(Fields.In, "FAX_NUMBER").getString(r);
	String E_MAIL_ADDRESS = get(Fields.In, "E_MAIL_ADDRESS").getString(r);
	String POSITION_format = get(Fields.In, "POSITION_format").getString(r);
	String INITIALS = get(Fields.In, "INITIALS").getString(r);
	String cts_company_pk = get(Fields.In, "cts_company_pk").getString(r);
	Long ResolutionModeInternal = get(Fields.In, "ResolutionModeInternal").getInteger(r);
	Long CONTACT_CODE = get(Fields.In, "CONTACT_CODE").getInteger(r);
		
	String existingData = get(Fields.In, "existingData").getString(r);
	try {
	Document doc = DocumentBuilderFactory.newInstance().newDocumentBuilder()
	       .parse(new InputSource(new StringReader(xml)));


	Node root = doc.getElementsByTagName("SUS_PERSON").item(0);
	//((Element)root).setAttribute("ExternalKey",Long.toString(CONTACT_CODE)); TODO should be DPD contact code in the future
	((Element)root).setAttribute("ProcessExistingData",existingData); 

	//add the company fk
	Element company=doc.createElement("T_SUS_PERSON_SUBSIDIARY_FK");
	((Element)company).setAttribute("ResolutionMode",Long.toString(ResolutionModeInternal));
	Element companyKey = doc.createElement("Existing_InternalKey");
	companyKey.appendChild(doc.createTextNode(cts_company_pk));
	company.appendChild(companyKey);
	root.appendChild(company);

	//person last name
	Element personName = doc.createElement("T_SUS_PERSON_Name_STR");
	personName.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(SURNAME_format)));
	root.appendChild(personName);
	


	//person phone (optional) T_SUS_PERSON_Phone1_STR
	if(TELEPHONE_NUMBER!=null &amp;&amp; TELEPHONE_NUMBER.length()>0){
		Element phone = doc.createElement("T_SUS_PERSON_Phone1_STR");
		phone.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(TELEPHONE_NUMBER)));
		root.appendChild(phone);
	}	

	// person fax (optional) T_SUS_PERSON_Fax_STR
	if(FAX_NUMBER!=null &amp;&amp; FAX_NUMBER.length()>0){
		Element fax = doc.createElement("T_SUS_PERSON_Fax_STR");
		fax.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(FAX_NUMBER)));
		root.appendChild(fax);
	}	
		

	// person email (opt)T_SUS_PERSON_Email1_STR
	if(E_MAIL_ADDRESS!=null &amp;&amp; E_MAIL_ADDRESS.length()>0){
		Element email = doc.createElement("T_SUS_PERSON_Email1_STR");
		email.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(E_MAIL_ADDRESS)));
		root.appendChild(email);
	}	

	//person comment T_SUS_PERSON_Comment_MEM
		Element comment = doc.createElement("T_SUS_PERSON_Comment_MEM");
		comment.appendChild(doc.createTextNode(Long.toString(CONTACT_CODE)));
		root.appendChild(comment);


	//person first name (optional) T_SUS_PERSON_FirstName_STR
	if(GIVEN_NAME_format!=null &amp;&amp; GIVEN_NAME_format.length()>0){
		Element givenName = doc.createElement("T_SUS_PERSON_FirstName_STR");
		givenName.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(GIVEN_NAME_format)));
		root.appendChild(givenName);
	}
	//person title (optional) T_SUS_PERSON_Title_STR
	if(POSITION_format!=null &amp;&amp; POSITION_format.length()>0){
		Element position = doc.createElement("T_SUS_PERSON_Title_STR");
		position.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(POSITION_format)));
		root.appendChild(position);
	}

	// person initials T_SUS_PERSON_Custom01_STR (optional)
	if(INITIALS!=null &amp;&amp; INITIALS.length()>0){
		Element initials = doc.createElement("T_SUS_PERSON_Custom01_STR");
		initials.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(INITIALS)));
		root.appendChild(initials);
	}	

	//person position T_SUS_PERSON_Custom02_STR

	
	Transformer transformer;
	
		transformer = TransformerFactory.newInstance().newTransformer();
		transformer.setOutputProperty(OutputKeys.OMIT_XML_DECLARATION, "yes");
		transformer.setOutputProperty(OutputKeys.METHOD,"xml");
		//initialize StreamResult with File object to save to file
		StreamResult result = new StreamResult(new StringWriter());
		DOMSource source = new DOMSource(doc);
		transformer.transform(source, result);
		 xmlString = result.getWriter().toString();
			
	
	// Set the value in the output field
	//
	
	} catch (Exception e) {
		logBasic("Exception for creating person xml");
	} 

	
		Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
	
		get(Fields.Out, "xmlPerson").setValue(outputRow, xmlString);
		putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
