	//String POST_OFFICE_BOX = get(Fields.In, "POST_OFFICE_BOX").getString(r);
	String existingData = get(Fields.In, "existingData").getString(r);
	try {
	Document doc = DocumentBuilderFactory.newInstance().newDocumentBuilder()
	       .parse(new InputSource(new StringReader(xml)));


	Node root = doc.getElementsByTagName("SUS_PERSON").item(0);
	//((Element)root).setAttribute("ExternalKey",company_externalKey);
	((Element)root).setAttribute("ProcessExistingData",existingData);

	//add the company fk
	Element company=doc.createElement("T_SUS_PERSON_SUBSIDIARY_FK");
	((Element)company).setAttribute("ResolutionMode",Long.toString(ResolutionModeInternal));
	Element companyKey = doc.createElement("Existing_InternalKey");
	companyKey.appendChild(doc.createTextNode(cts_company_pk));
	company.appendChild(companyKey);
	root.appendChild(company);

	Element personName = doc.createElement("T_SUS_SUBSIDIARY_Name_STR");
	personName.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(GIVEN_NAME_format+" "+SURNAME_format)));
	root.appendChild(personName);



	//Element street = doc.createElement("T_SUS_SUBSIDIARY_Street_STR");
	//street.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(STREET_NAME)));
	//root.appendChild(street);
	
	
	Transformer transformer;
	
		transformer = TransformerFactory.newInstance().newTransformer();
		transformer.setOutputProperty(OutputKeys.OMIT_XML_DECLARATION, "yes");
		transformer.setOutputProperty(OutputKeys.METHOD,"xml");
		//initialize StreamResult with File object to save to file
		StreamResult result = new StreamResult(new StringWriter());
		DOMSource source = new DOMSource(doc);
		transformer.transform(source, result);
		 xmlString = result.getWriter().toString();
			
	
	// Set the value in the output field
	//
	
