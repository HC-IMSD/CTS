import java.io.IOException;
import java.io.StringReader;
import java.io.StringWriter;

import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import javax.xml.parsers.ParserConfigurationException;
import javax.xml.transform.OutputKeys;
import javax.xml.transform.Transformer;
import javax.xml.transform.TransformerConfigurationException;
import javax.xml.transform.TransformerException;
import javax.xml.transform.TransformerFactory;
import javax.xml.transform.TransformerFactoryConfigurationError;
import javax.xml.transform.dom.DOMSource;
import javax.xml.transform.stream.StreamResult;


import org.w3c.dom.CharacterData;
import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;
import org.xml.sax.InputSource;
import org.apache.commons.lang3.StringEscapeUtils;


String xml="&lt;SUS_SUBSIDIARY>&lt;/SUS_SUBSIDIARY>";
String xmlString;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();


    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

			

	String STREET_NAME = get(Fields.In, "STREET_NAME_format").getString(r);
	Long ResolutionModeExternal = get(Fields.In, "ResolutionModeExternal").getInteger(r);
	Long ResolutionModeInternal = get(Fields.Out, "ResolutionModeInternal").getInteger(r);
	String COMPANY_NAME = get(Fields.In, "COMPANY_NAME_format").getString(r);
	String cts_LOC_LOCATION_pk = get(Fields.In, "cts_LOC_LOCATION_pk").getString(r);
	String company_externalKey = get(Fields.In, "company_externalKey").getString(r);
	String POST_OFFICE_BOX = get(Fields.In, "POST_OFFICE_BOX").getString(r);
	String existingData = get(Fields.In, "existingData").getString(r);
	try {
	Document doc = DocumentBuilderFactory.newInstance().newDocumentBuilder()
	       .parse(new InputSource(new StringReader(xml)));


	Node root = doc.getElementsByTagName("SUS_SUBSIDIARY").item(0);
	((Element)root).setAttribute("ExternalKey",company_externalKey);
	((Element)root).setAttribute("ProcessExistingData",existingData);

	Element companyName = doc.createElement("T_SUS_SUBSIDIARY_Name_STR");
	companyName.appendChild(doc.createTextNode(COMPANY_NAME));
	root.appendChild(companyName);
	Element location=doc.createElement("T_SUS_SUBSIDIARY_LOCATION1_FK");
	((Element)location).setAttribute("ResolutionMode",Long.toString(ResolutionModeInternal));
	Element locationKey = doc.createElement("Existing_InternalKey");
	locationKey.appendChild(doc.createTextNode(cts_LOC_LOCATION_pk));
	location.appendChild(locationKey);
	root.appendChild(location);
	if( POST_OFFICE_BOX!=null &amp;&amp; POST_OFFICE_BOX.length()>0){
		
		Element poBox = doc.createElement("T_SUS_SUBSIDIARY_POBox_STR");
		poBox.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(POST_OFFICE_BOX)));
		root.appendChild(poBox);
	}

	Element street = doc.createElement("T_SUS_SUBSIDIARY_Street_STR");
	street.appendChild(doc.createTextNode(StringEscapeUtils.escapeXml(STREET_NAME)));
	root.appendChild(street);
	
	
	Transformer transformer;
	
		transformer = TransformerFactory.newInstance().newTransformer();
		transformer.setOutputProperty(OutputKeys.OMIT_XML_DECLARATION, "yes");
		transformer.setOutputProperty(OutputKeys.METHOD,"xml");
		//initialize StreamResult with File object to save to file
		StreamResult result = new StreamResult(new StringWriter());
		DOMSource source = new DOMSource(doc);
		transformer.transform(source, result);
		 xmlString = result.getWriter().toString();
			
	
	// Set the value in the output field
	//
	
	} catch (Exception e) {
		logBasic("Exception");
	} 

	
			Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
	
			get(Fields.Out, "xmlCompany2").setValue(outputRow, xmlString);
		putRow(data.outputRowMeta, outputRow);

  

	return true;
}</class_source>
