String xmlRoot="&lt;SUB_REFSOURCE>&lt;/SUB_REFSOURCE>";
String xmlString;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();


    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}
	
	


	String ref_value = get(Fields.In, "ref_value").getString(r);
	Long processExisting_overwrite = get(Fields.In, "processExisting_overwrite").getInteger(r);
	Long ResolutionMode_external = get(Fields.In, "ResolutionMode_external").getInteger(r);

	try {
	Document doc = DocumentBuilderFactory.newInstance().newDocumentBuilder()
	       .parse(new InputSource(new StringReader(xmlRoot)));

	Node root = doc.getElementsByTagName("SUB_REFSOURCE").item(0);
	
	//create the root
	((Element)root).setAttribute("ExternalKey",ref_value);
	((Element)root).setAttribute("ProcessExistingData",Long.toString(processExisting_overwrite));
	
	Element src = doc.createElement("T_SUB_REFSOURCE_Description_STR");
	src.appendChild(doc.createTextNode(ref_value));
	root.appendChild(src);

	
	src = doc.createElement("T_SUB_REFSOURCE_Comment_MEM");
	src.appendChild(doc.createTextNode(ref_value));
	root.appendChild(src);


	//create xml string. Keep in mind of extended character set
	
	Transformer transformer;
	
		transformer = TransformerFactory.newInstance().newTransformer();
		transformer.setOutputProperty(OutputKeys.OMIT_XML_DECLARATION, "yes");
		transformer.setOutputProperty(OutputKeys.METHOD,"xml");
		//initialize StreamResult with File object to save to file
		StreamResult result = new StreamResult(new StringWriter());
		DOMSource source = new DOMSource(doc);
		transformer.transform(source, result);
		 xmlString = result.getWriter().toString();
			
	
	// Set the value in the output field
	//
	
	} catch (Exception e) {
		logBasic("Exception");
	} 
	
	
			Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
	
			get(Fields.Out, "xmlOut").setValue(outputRow, xmlString);
		putRow(data.outputRowMeta, outputRow);

  

	return true;
}</class_source>
