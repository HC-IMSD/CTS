        <class_source>


import java.io.IOException;
import java.io.StringReader;
import java.io.StringWriter;
import java.util.UUID;


import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import javax.xml.parsers.ParserConfigurationException;
import javax.xml.transform.OutputKeys;
import javax.xml.transform.Transformer;
import javax.xml.transform.TransformerConfigurationException;
import javax.xml.transform.TransformerException;
import javax.xml.transform.TransformerFactory;
import javax.xml.transform.TransformerFactoryConfigurationError;
import javax.xml.transform.dom.DOMSource;
import javax.xml.transform.stream.StreamResult;


import org.w3c.dom.CharacterData;
import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;
import org.xml.sax.InputSource;
import org.apache.commons.lang3.StringEscapeUtils;


String xml="&lt;T_SUB_SUBSTANCETRANSLATION>&lt;/T_SUB_SUBSTANCETRANSLATION>";
String xmlRoot="&lt;SUB_SUBSTANCE>&lt;/SUB_SUBSTANCE>";
String xmlString;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();


    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}
	
	
	String ACTIVE_INGREDIENT_DESC_F = get(Fields.In, "ACTIVE_INGREDIENT_DESC_F").getString(r);
	Long row_sequnce = get(Fields.In, "row_seq").getInteger(r);
	String cts_lang_pk = get(Fields.In, "cts_lang_pk").getString(r);
	Long ResolutionMode_overwrite = get(Fields.In, "ResolutionMode_overwrite").getInteger(r);
	
	String ACTIVE_INGREDIENT_DESC = get(Fields.In, "ACTIVE_INGREDIENT_DESC").getString(r);
	String active_desc_formatted = get(Fields.In, "active_desc_formatted").getString(r);
	String CAS_NUMBER = get(Fields.In, "CAS_NUMBER").getString(r);
	String NOTES = get(Fields.In, "NOTES").getString(r);
	Long ACTIVE_INGREDIENT_CODE = get(Fields.In, "ACTIVE_INGREDIENT_CODE").getInteger(r);
	
	String aliases = get(Fields.In, "aliases").getString(r);
	
	String InternalKey = get(Fields.In, "InternalKey").getString(r);

	//if(ACTIVE_INGREDIENT_DESC_F!=null &amp;&amp; ACTIVE_INGREDIENT_DESC_F.length()>0){
		//	setOutputDone();
		//return true;

	

	try {
	Document doc = DocumentBuilderFactory.newInstance().newDocumentBuilder()
	       .parse(new InputSource(new StringReader(xmlRoot)));

	Node root = doc.getElementsByTagName("SUB_SUBSTANCE").item(0);
	Element rootTrans=null;
	if(ACTIVE_INGREDIENT_DESC_F!=null &amp;&amp; ACTIVE_INGREDIENT_DESC_F.length()>0){
 		rootTrans = doc.createElement("T_SUB_SUBSTANCETRANSLATION");
		rootTrans.setAttribute("ItemNo",Long.toString(row_sequnce));
	
		Element lang_fk= doc.createElement("T_SUB_SUBSTANCETRANSLATION_LANGUAGE_FK");
			lang_fk.setAttribute("ResolutionMode",Long.toString(ResolutionMode_overwrite));
	//lang_fk.appendChild(doc.createTextNode(Long.toString(ResolutionMode_overwrite)));


		Element existingLangCode = doc.createElement("Existing_InternalKey");

		existingLangCode.appendChild(doc.createTextNode(cts_lang_pk));
		lang_fk.appendChild(existingLangCode);

		rootTrans.appendChild(lang_fk);
		Element lang_text= doc.createElement("T_SUB_SUBSTANCETRANSLATION_Translation_STR");
		lang_text.appendChild(doc.createTextNode(ACTIVE_INGREDIENT_DESC_F));
		rootTrans.appendChild(lang_text);

	}
	//create the root
	((Element)root).setAttribute("ExternalKey",Long.toString(ACTIVE_INGREDIENT_CODE));
	((Element)root).setAttribute("ProcessExistingData",Long.toString(ResolutionMode_overwrite));
	Element substanceName = doc.createElement("T_SUB_SUBSTANCE_Name_STR");
	//substanceName.appendChild(doc.createTextNode(ACTIVE_INGREDIENT_DESC));
	substanceName.appendChild(doc.createTextNode(active_desc_formatted));
	
	//if(CAS_NUMBER!=null){
		Element casVal = doc.createElement("T_SUB_SUBSTANCE_Cas_STR");
		casVal.appendChild(doc.createTextNode(CAS_NUMBER));
	//}
	//if(NOTES!=null){
		Element noteVal = doc.createElement("T_SUB_SUBSTANCE_Comment_MEM");
		noteVal.appendChild(doc.createTextNode(NOTES));
	//}

	
	//create the ordered structure
	root.appendChild(substanceName);
	root.appendChild(casVal);
	root.appendChild(noteVal);

	
	//create alias Node(s). Must be before any translations
	
	String[] aliasArray = aliases.split(",");
	int numAliases=aliasArray.length-1; // ignore the firs


	if(aliasArray.length-1>0){

		//create the reference source xml
			
			
	
		for(int i=1;i&lt;aliasArray.length;i++){

			//inefficient but appears to run in paralell?
			Element rootRefSource = doc.createElement("T_SUB_SUBSTANCEALIAS_REFSOURCE_FK");
				rootRefSource.setAttribute("ResolutionMode","2"); //existing internal TODO hard coded
			Element refInternal= doc.createElement("Existing_InternalKey");
			refInternal.appendChild(doc.createTextNode(InternalKey)); 	
			rootRefSource.appendChild(refInternal);	


			Element rootAlias = doc.createElement("T_SUB_SUBSTANCEALIAS");
			rootAlias.setAttribute("ItemNo",""+i);
	
			Element aliasName= doc.createElement("T_SUB_SUBSTANCEALIAS_Aliasname_STR");
			aliasName.appendChild(doc.createTextNode(aliasArray[i]));
			
			Element aliasGuid= doc.createElement("T_SUB_SUBSTANCEALIAS_AliasGuid_STR");
			aliasGuid.appendChild(doc.createTextNode(""+UUID.randomUUID())); 
			logBasic("Looop "+i);
			logBasic("This is the alias"+rootRefSource);
			rootAlias.appendChild(aliasName);
			rootAlias.appendChild(rootRefSource);
			rootAlias.appendChild(aliasGuid);				

			root.appendChild(rootAlias);

		}
	}

	
	if(rootTrans!=null) root.appendChild(rootTrans);

	//create xml string. Keep in mind of extended character set
	
	Transformer transformer;
	
		transformer = TransformerFactory.newInstance().newTransformer();
		transformer.setOutputProperty(OutputKeys.OMIT_XML_DECLARATION, "yes");
		transformer.setOutputProperty(OutputKeys.METHOD,"xml");
		//initialize StreamResult with File object to save to file
		StreamResult result = new StreamResult(new StringWriter());
		DOMSource source = new DOMSource(doc);
		transformer.transform(source, result);
		 xmlString = result.getWriter().toString();
			
	
	// Set the value in the output field
	//
	
	} catch (Exception e) {
		logBasic("Exception");
	} 
	
	
			Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
	
			get(Fields.Out, "xmlSubstance").setValue(outputRow, xmlString);
		putRow(data.outputRowMeta, outputRow);

  

	return true;
}</class_source>
